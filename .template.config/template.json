{
    "$schema": "http://json.schemastore.org/template",
    "author": "Fabian Juette",
    "classifications": ["REST", "Api", "DDD", "C# 12", ".NET 8"],
    "identity": "FJuette.Template.WebApi",
    "groupIdentity": "FJuette.Template",
    "name": "MS REST-Api template in ddd style",
    "shortName": "ddd-webapi",
    "tags": {
        "language": "C#",
        "type": "project"
    },
    "sourceName": "Tusk",
    "preferNameDirectory": true,
    "sources": [
        {
            "modifiers": [
                {
                    "exclude": [
                        ".vs/**",
                        ".git/**",
                        "**/bin/**",
                        "**/obj/**",
                        ".idea/**",
                        "*.nupkg",
                        "*.nuspec",
                        "package/**",
                        "_rels/**",
                        "Tusk.v3.ncrunchsolution.user",
                        "RabbitMQ.adoc",
                        "DEVELOPMENT.adoc",
                        "*.drawio.*",
                        "[Content_Types].xml"
                    ],
                    "include": [
                      "**/src/*",
                      "**/tests/*",
                      ".dockerignore",
                      ".editorconfig",
                      ".gitignore",
                      ".releaserc",
                      "Architecture.png",
                      "Dockerfile",
                      "VERSIONING.adoc",
                      "README.md",
                      "LICENSE",
                      "*.sln"
                    ]
                }
            ]
        }
    ],
    "symbols": {
        "DisableAuthentication": {
            "type": "parameter",
            "datatype": "bool",
            "defaultValue": "false"
        }
    }
}
