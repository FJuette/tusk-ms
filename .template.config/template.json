{
    "$schema": "http://json.schemastore.org/template",
    "author": "Fabian Juette",
    "classifications": ["REST", "Api", "DDD", "C#11", ".net7.0"],
    "identity": "FJuette.Template.WebApi",
    "groupIdentity": "FJuette.Template",
    "name": "MS WebApi template in ddd style",
    "shortName": "ddd-webapi",
    "tags": {
        "language": "C#",
        "type": "project"
    },
    "sourceName": "Tusk",
    "preferNameDirectory": true,
    "sources": [
        {
            "modifiers": [
                {
                    "exclude": [
                        ".vs/**",
                        ".git/**",
                        "bin/**",
                        "obj/**",
                        ".idea/**",
                        "*.nupkg",
                        "*.nuspec",
                        "Tusk.v3.ncrunchsolution.user",
                        "RabbitMQ.adoc"
                    ]
                }
            ]
        }
    ],
    "symbols": {
        "DisableAuthentication": {
            "type": "parameter",
            "datatype": "bool",
            "defaultValue": "false"
        }
    }
}
